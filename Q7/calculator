n1 = float(input())
operador ='B'
while operador != '&':
    n2 = float(input())
    operador=input()
    try:
        if operador == '+':
            n1=n1+n2
            print('%.3f' %n1)
        elif operador == '*':
            n1 = n1*n2
            print('%.3f'%n1)
        elif operador == '/':
            n1 = n1/n2
            print('%.3f'%n1)
        elif operador == '-':
            n1 = n1-n2
            print('%.3f'%n1)
    except:
        print("operacao nao pode ser realizada")